<h1>Listing stop_types</h1>

<table>
  <tr>
    <th>Name</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @stop_types.each do |stop_type| %>
  <tr>
    <td><%= stop_type.name %></td>
    <td><%= link_to 'Show', stop_type %></td>
    <td><%= link_to 'Edit', edit_stop_type_path(stop_type) %></td>
    <td><%= link_to 'Destroy', stop_type, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

